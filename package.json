{
    "name": "robbybro",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/material-ui": "^0.21.1",
        "material-ui": "^0.20.0",
        "node-sass-chokidar": "^1.2.2",
        "normalize.css": "^8.0.0",
        "npm-run-all": "^4.1.2",
        "react": "^16.3.1",
        "react-dom": "^16.3.1",
        "react-router": "^4.3.1",
        "react-router-dom": "^4.3.1",
        "react-scripts-ts": "2.14.0",
        "react-speed-dial": "^0.4.7"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-ts": "react-scripts-ts start",
        "start": "npm-run-all -p watch-css start-ts",
        "build-ts": "react-scripts-ts build",
        "build": "npm-run-all build-css build-ts",
        "deploy": "npm run build&&gh-pages -d build",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject"
    },
    "devDependencies": {
        "@types/jest": "^22.2.2",
        "@types/node": "^9.6.4",
        "@types/react": "^16.3.8",
        "@types/react-dom": "^16.0.5",
        "@types/react-router-dom": "^4.3.1",
        "gh-pages": "^1.1.0",
        "typescript": "^2.8.1"
    },
    "homepage": "http://robbybrosman.com/"
}
